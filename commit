p=$(basename $0)

case $# in
0)
  echo "usage: $p message"
  message="not significative"
  ;;
1)
  message=$1
  ;;
esac

#svn ci -m "$message"
#./.github
git commit -a -m "$message"
git push origin master
#./.gitlab
#git commit -a -m "$message"
#git push origin master
#./.github

touch git.log
echo "$(date)	$message" >> git.log
